<div class="outer-pad">

  <section class="itswd-section itswd-section-full-width">
    <div class="itswd-section-content itswd-section-content-constrained">
      <div class="home-social--gray">
        <div class="home-social--gray__headline-icons-wrap">
          <h2 class="home-social--gray__headline">{{ social.headline }}</h2>
          <ul class="home-social--gray__social-links">
            <li>
              <a href="#" target="_blank">
                <i class="fak fa-iastate22-instagram" aria-hidden="true"></i>
                <span class="visible-for-screen-readers">Instagram</span>
              </a>
            </li>
            <li>
              <a href="#" target="_blank">
                <i class="fa-brands fa-youtube" aria-hidden="true"></i>
                <span class="visible-for-screen-readers">YouTube</span>
              </a>
            </li>
            <li>
              <a href="#" target="_blank">
                <i class="fa-brands fa-facebook" aria-hidden="true"></i>
                <span class="visible-for-screen-readers">Facebook</span>
              </a>
            </li>
            <li>
              <a href="#" target="_blank">
                <i class="fa-brands fa-twitter" aria-hidden="true"></i>
                <span class="visible-for-screen-readers">Twitter</span>
              </a>
            </li>
          </ul>
        </div>
        <div class="home-social--gray__images-wrap">
          <div class="home-social--gray__images-image">
            <img src="{{ social.images.one|path }}" alt="" width="301" height="301" />
          </div>
          <div class="home-social--gray__images-image">
            <img src="{{ social.images.two|path }}" alt="" width="301" height="301" />
          </div>
          <div class="home-social--gray__images-image">
            <img src="{{ social.images.three|path }}" alt="" width="301" height="301" />
          </div>
        </div>
      </div>
    </div>
  </section>
</div>